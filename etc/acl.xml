<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="MyModules_QuickOrder::all" title="Quick order all" translate="title" sortOrder="30" >
                    <resource id="MyModules_QuickOrder::orderAll" title="Quick Order" translate="title" sortOrder="10" >
                        <resource
                                id="MyModules_QuickOrder::delete_order"
                                title="Quick Order Delete"
                                translate="title"
                                sortOrder="30"
                        />
                        <resource
                                id="MyModules_QuickOrder::edit_order"
                                title="Quick Order Edit"
                                translate="title"
                                sortOrder="40"
                        />
                        <resource
                                id="MyModules_QuickOrder::inline_order"
                                title="Quick Order inline"
                                translate="title"
                                sortOrder="50"
                        />
                    </resource>
                    <resource id="MyModules_QuickOrder::statusAll" title="Status" translate="title" sortOrder="10" >
                        <resource
                                id="MyModules_QuickOrder::delete_status"
                                title="Status Delete"
                                translate="title"
                                sortOrder="30"
                        />
                        <resource
                                id="MyModules_QuickOrder::inline_status"
                                title="Status Inline Edit"
                                translate="title"
                                sortOrder="50"
                        />
                        <resource
                                id="MyModules_QuickOrder::create_status"
                                title="Status Create"
                                translate="title"
                                sortOrder="50"
                        />
                        <resource
                                id="MyModules_QuickOrder::config_status"
                                title="Status Config"
                                translate="title"
                                sortOrder="50"
                        />
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
